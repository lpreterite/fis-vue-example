{
  "name": "fis-vue-example",
  "version": "0.2.0",
  "description": "基于fis构建的vuejs事例（包含vue-router的使用）",
  "main": "app.js",
  "scripts": {
    "dev": "fis3 release debug -w -d ../release/fis-vue-example",
    "publish": "fis3 release -d ../release/fis-vue-example-publish",
    "server": "fis3 server start -p 8808 --root ../release/fis-vue-example",
    "qa": "fis3 release qa -w"
  },
  "repository": {
    "type": "git",
    "url": "https://git.coding.net/packy/fis-vue-example.git"
  },
  "keywords": [
    "vue",
    "fis",
    "vue-router"
  ],
  "devDependencies": {    
    "fis-parser-node-sass": "^0.2.1",
    "fis3": "^3.3.16",
    "fis3-hook-amd": "^0.1.1",
    "fis3-packager-deps-pack": "^0.1.2",
    "fis3-postpackager-loader": "^1.3.2",
    "fis3-postprocessor-autoprefixer": "^1.0.0"
  },
  "author": "packy <lpreterite@126.com>",
  "license": "MIT"
}
